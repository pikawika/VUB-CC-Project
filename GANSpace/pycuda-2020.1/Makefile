.PHONY	: all install clean tags dist userdoc devdoc
# DO NOT EDIT THIS FILE -- it was generated by configure.py
# configure.py --cuda-root=/usr/local/cuda-10.1 --cuda-enable-gl

all: tags
	/home/lennert/anaconda3/envs/ganspace/bin/python setup.py build

dist:
	/home/lennert/anaconda3/envs/ganspace/bin/python setup.py sdist

install: tags
	/home/lennert/anaconda3/envs/ganspace/bin/python setup.py install

clean:
	rm -Rf build
	rm -f tags

tags:
	ctags -R src || true

tests:
	echo "running tests"
	find ./test -type f -name "*.py" -exec python {} \;
